#include <stdio.h>

int main() {
    int n,r,rev=0,t;
    printf("\nEnter an integer value:");
    scanf("%d",&n);
    t=n;
    while(n>0)
    {
        r=n%10;
        rev=rev*10+r;
        n=n/10;
}
printf("\n The Reverse of %d is %d",t,rev);
}
//out put//
Enter an integer value:63

 The Reverse of 63 is 36

=== Code Execution Successful ===
